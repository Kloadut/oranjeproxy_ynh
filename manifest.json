{
    "package_format": 1,
    "name": "OranjeProxy",
    "id": "oranjeproxy",
    "description": {
        "en": "A simple PHP proxy",
        "fr": "Un proxy PHP lÃ©ger"
    },
    "url": "http://lehollandaisvolant.net/tout/oranjeproxy/",
    "maintainer": {
        "name": "kload",
        "email": "kload@kload.fr"
    },
    "multi_instance": "false",
       "requirements": {
        "yunohost": ">= 2.4.0"
    },
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for OranjeProxy",
                    "fr": "Choisissez un domaine pour OranjeProxy"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for OranjeProxy",
                    "fr": "Choisissez un chemin pour OranjeProxy"
                },
                "example": "/proxy",
                "default": "/proxy"
            }
        ]
    }
}
